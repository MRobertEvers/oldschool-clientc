{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug with MinGW64 (osx.exe)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-mingw/osx.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/build-mingw",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
            "sourceFileMap": {
                "C:/Users/mrobe/Documents/git_repos/3d-raster": "${workspaceFolder}",
                "c:/Users/mrobe/Documents/git_repos/3d-raster": "${workspaceFolder}",
                "/c/Users/mrobe/Documents/git_repos/3d-raster": "${workspaceFolder}",
                "/c/users/mrobe/documents/git_repos/3d-raster": "${workspaceFolder}",
                "C:\\Users\\mrobe\\Documents\\git_repos\\3d-raster": "${workspaceFolder}",
                "..": "${workspaceFolder}",
                "../..": "${workspaceFolder}"
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                },
                {
                    "description": "Set substitute-path for MSYS2 Unix-style paths",
                    "text": "-gdb-set substitute-path /c/Users/mrobe/Documents/git_repos/3d-raster C:/Users/mrobe/Documents/git_repos/3d-raster",
                    "ignoreFailures": true
                },
                {
                    "description": "Set substitute-path for MSYS2 lowercase paths",
                    "text": "-gdb-set substitute-path /c/users/mrobe/documents/git_repos/3d-raster C:/Users/mrobe/Documents/git_repos/3d-raster",
                    "ignoreFailures": true
                },
                {
                    "description": "Set substitute-path for Windows backslash paths",
                    "text": "-gdb-set substitute-path C:\\Users\\mrobe\\Documents\\git_repos\\3d-raster C:/Users/mrobe/Documents/git_repos/3d-raster",
                    "ignoreFailures": true
                },
                {
                    "description": "Set substitute-path for Windows forward slash paths (lowercase)",
                    "text": "-gdb-set substitute-path c:/Users/mrobe/Documents/git_repos/3d-raster C:/Users/mrobe/Documents/git_repos/3d-raster",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "",
            "logging": {
                "moduleLoad": false,
                "trace": false,
                "engineLogging": false,
                "programOutput": true,
                "exceptions": true
            }
        }
    ]
}